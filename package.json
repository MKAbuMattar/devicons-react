{
  "name": "devicons-react",
  "version": "1.1.2",
  "description": "Programming icons",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "scripts": {
    "clean:lib": "rm -rf lib",
    "clean:svg": "rm -rf svg",
    "clean:svgr": "rm -rf src/icons",
    "clean:svgr:index": "rm ./src/icons/index.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:svg": "node ./build/build.config.js && npm run clean:svg && node ./build/generate.js",
    "build:svgr": "npm run clean:svgr && npx @svgr/cli  --out-dir src/icons --index-template index-template.js -- svg && npm run clean:svgr:index",
    "build:lib": "npm run clean:lib && rollup -c",
    "build": "npm run build:svg && npm run build:svgr && npm run build:lib",
    "publish:lib": "npm run build && cd lib && npm publish"
  },
  "author": "Mohammad Abu Mattar",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@storybook/addon-actions": "^6.4.15",
    "@storybook/addon-essentials": "^6.4.15",
    "@storybook/addon-links": "^6.4.15",
    "@storybook/react": "^6.1.21",
    "@svgr/cli": "^6.2.0",
    "babel-loader": "^8.2.2",
    "devicon": "^2.14.0",
    "fs-plus": "^3.1.1",
    "node-recursive-directory": "^1.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "svgson": "^5.2.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MKAbuMattar/devicons-react.git"
  },
  "keywords": [
    "programming",
    "icons"
  ],
  "bugs": {
    "url": "https://github.com/MKAbuMattar/devicons-react/issues"
  },
  "homepage": "https://devicons-react.netlify.app/"
}
